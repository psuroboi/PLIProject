 R021_UPD: PROC;                                                        06300012
                                                                        06310012
                                               /* SOC RB01254        */ 06320012
   NFI_AREA = FILE_RCBDTC1, BY NAME;                                    06330012
   NFI_AREA.LAST_PROCESSED_DATE = WRK_RUN_DATE;                         06340012
                                                                        06350012
 /* WRITE RECORD TO NEW FULL IMAGE FILE                              */ 06360012
