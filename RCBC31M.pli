                    :TS_REC.RULE :NUL690J;                 /*RB01298*/

   IF CNTRY_TM_FLAG = TRUE THEN
     EXEC SQL  FETCH CTM_0690
               INTO :TS_REC.CTRY_NO,
                    :TS_REC.P_TYPE,
                    :TS_REC.MODEL,
                    :TS_REC.B_WARR,
                    :TS_REC.SLVLCD :NUL690E,   /* CEDS00000384 */
                    :TS_REC.EFF_DATE,
