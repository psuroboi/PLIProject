   IF (RCBM31I.C_FW_SELI  ¬= ''
    &  RCBM31I.TYPE_SELI  ¬= '') THEN
   DO;
   RCBM31I.C_FW_SELL = -1;
   RCBM31I.TYPE_SELL = -1;
