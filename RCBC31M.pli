   END;

   IF OK_FLAG = FALSE THEN
   DO;
     RCBM31I.COUNTRYL        = -1;
     RCBM31O.COUNTRYC        = DFHRED;
     WRK_COUNTRY_NUMBER      = '001';
     CALL RCBC31M_CLEAR_MAP_2;
