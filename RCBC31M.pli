   IF ASCA_STOP_FLAG = TRUE THEN
   DO;
     RCBM31O.COUNTRYC        = DFHRED;
     RCBM31I.COUNTRYL        = -1;
     WRK_COUNTRY_NUMBER      = '001';
     CALL RCBC31M_CLEAR_MAP_2;
     RCBM31O.MSGLNBO         = 'O079 ' || ERR_O079;
     CALL RCBC31M_SEND_MAP_CSR;
