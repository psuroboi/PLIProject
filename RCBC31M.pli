1/********************************************************************/
 /*      VALIDATE MAP RCBM31                                         */
 /********************************************************************/
0RCBC31M_VALIDATE_MAP: PROC;

 PRODUCT_TYPE_ENT  = 'N';
 MODEL_NUMBER_ENT  = 'N';
 PRODUCT_TYPE_SEL  = '';
 MODEL_NUMBER_SEL  = '';
 STAND_FW_FLAG     = FALSE;
 STAND_T_FLAG      = FALSE;
 STAND_TM_FLAG     = FALSE;
 CNTRY_FLAG        = FALSE;
