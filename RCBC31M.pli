 IF WRK_COUNTRY_NUMBER ¬= RCBM31I.COUNTRYI THEN
 DO;
   SEL_OPT = '9';
   WRK_COUNTRY_NUMBER = RCBM31I.COUNTRYI;

   EXEC CICS LINK
      PROGRAM('RCBC99P')
      COMMAREA(COMMAREA)
      LENGTH(CSTG(COMMAREA));

   IF CTY_OK_FLAG = FALSE THEN
   DO;
     RCBM31I.COUNTRYL = -1;
