  IF RCBM31I.RULEI(I) ¬= 'FW' & RCBM31I.RULEI(I) ¬= 'HB' THEN
  DO;
     RCBM31O.MSGLNBO      = 'O189 ' ||  ERR_O189;
     RCBM31O.RULEC(I) = DFHRED;
     RCBM31I.RULEL(I) = -1;
