   DO;
   RCBM31I.S_FW_SELL = -1;
   RCBM31I.TYPE_SELL = -1;
   RCBM31I.MODL_SELL = -1;
   RCBM31O.MSGLNBO   = 'O174 ' || ERR_O174;
