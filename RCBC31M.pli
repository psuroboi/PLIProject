  IF RCBM31I.RULEI(I)  = 'HB' & RCBM31I.SLC2I(I)  = ' ' THEN
  DO;
     RCBM31O.MSGLNBO      = 'O190 ' ||  ERR_O190;
     RCBM31O.SLC2C(I) = DFHRED;
     RCBM31I.SLC2L(I) = -1;
