 /* MODULE NAME      : PROCESS_CA9                                   */
 /*                                                                  */
 /* DESCRIPTION      : PROCESS CA9 FILES                             */
 /********************************************************************/
 PROCESS_CA9: PROC;

0DO WHILE (SSK5.EOF = '0'
        &  SSK5.REC_TYPE ¬= '99');    /* WHILE NOT EOF                */
0    CALL RCBBCZM_INIT_COUNTRY;
0  DO WHILE (SUBSTR (STRING (PREV_SSK5), 1, 6)  /* WHILE SAME COUNTRY */
             = SUBSTR (STRING (SSK5), 1, 6));
      SELECT;
       WHEN(STRING(SK18) < STRING(SK19))  /* SOURCE S1 SMALLEST      */
          DO;
           CALL R030_INSERT_RCBBS16;
           CALL RCBBCZM_CALC1;
           CALL R010_READ0;
          END;
       WHEN(STRING(SK18) > STRING(SK19))  /* SOURCE S2 SMALLEST      */
          DO;
           CALL R031_INSERT_RCBBS26;
           CALL RCBBCZM_CALC2;
           CALL R011_READ1;
          END;
       OTHERWISE DO;                 /* KEYS ARE EQUAL               */
           CALL R030_INSERT_RCBBS16;
           CALL RCBBCZM_CALC1;
           CALL R010_READ0;
           CALL RCBBCZM_CALC2;
           CALL R011_READ1;
          END;
      END;

     CALL RCBBCZM_SIC;               /* SET ITERATION CONDITIONS     */
   END;
0      CALL RCBBCZM_END_COUNTRY;                      /* END COUNTRY */
0END;

 END PROCESS_CA9;
