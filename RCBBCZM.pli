1/********************************************************************/
 /*                                                                  */
 /* MODULE NAME      : RCBBCZM_SAVE_HASH                             */
 /*                                                                  */
 /* DESCRIPTION      : SAVE HASH TOTAL INFORMATION ON PARAMETER.     */
 /*                    IF THE PARAMETER DOES NOT YET EXIST, IT WILL  */
 /*                    BE INSERTED.                                  */
 /*                                                                  */
 /*                    ISSUE MESSAGE 0173                            */
 /*                                                                  */
 /********************************************************************/
0 RCBBCZM_SAVE_HASH : PROC;
                                     /* INIT TABLESTRUCTURE          */
 TB_RCBV500                  = '';
 TB_RCBV500_I                = '';
 TB_RCBP24S.NUMBER           = 'RCB024';
 TB_RCBP24S.PROC_ID          = RCBPROC_REC.PROC_ID;
 TB_RCBP24S.CTRYNO           = PREV_SSK.CTRYNO;
 TB_RCBP24S.FILE_NAME        = WRK_FILE_ACT;
 TB_RCBP24S.SEQ_NUMBER       = '01';
 TB_RCBP24S.TIME_STAMP       = WRK_DATE_TIME;
 TB_RCBP24S.HASH_TOTAL       = COUNT.RCBBCA0_HASH;
 TB_RCBP24S.LENGTH_PARM      = WRK_LENGTH_RCB024;

0CALL RCBG50M (ADDR(TB_RCBV500),    /* UPDATE PARAMETER RCB024    */
               ADDR(TB_RCBV500_I),
               PTR_RET_AREA_BB,
               PTR_INT_AREA);
0SELECT(PARM_RET_AREA_BB.RETURN_CODE);
   WHEN (0);
   WHEN (4)                          /* WHEN NOT FOUND THEN INSERT  */
     DO;
       CALL RCBF50M (ADDR(TB_RCBV500),
                     ADDR(TB_RCBV500_I),
                     PTR_RET_AREA_BB,
                     PTR_INT_AREA);
       SELECT(PARM_RET_AREA_BB.RETURN_CODE);
         WHEN (0);
         OTHERWISE
           DO;
             CALL RCBBCZM_ERROR (ADDR (PARM_RET_AREA_BB.RETURN_SQL),
                                 'DB2',
                                 '#116');
           END;
       END;
     END;
   OTHERWISE
      DO;
        CALL RCBBCZM_ERROR (ADDR (PARM_RET_AREA_BB.RETURN_SQL),
                            'DB2',
                            '#117');
      END;
 END;

 PARM_INT_AREA.MESSAGE_TYPE     = 'IS';
 PARM_INT_AREA.MESSAGE_NUMBER   = '0173';
 PARM_INT_AREA.MSG_VAR_CODES    = '';
 PARM_INT_AREA.MSG_VAR_CODE_6   = 'FL';
 PARM_INT_AREA.MSG_VAR_VALUE_6  = WRK_FILE_ACT;
 PARM_INT_AREA.MSG_VAR_CODE_7   = 'TP';
 PARM_INT_AREA.MSG_VAR_VALUE_7  = WRK_DATE_TIME;
 PARM_INT_AREA.MSG_VAR_CODE_8   = 'HT';
 CNT_PIC15                      = COUNT.RCBBCA0_HASH;
 PARM_INT_AREA.MSG_VAR_VALUE_8  = CNT_PIC15;
 CALL RCB0M1M (PTR_INT_AREA);

0END RCBBCZM_SAVE_HASH;

