 /*                                                                  */
 /* DESCRIPTION      : PROCESS THE FILES CA0,CA2,CA5,CA8,CAB FILES   */
 /********************************************************************/
 PROCESS_CA0_2_5_8_B: PROC;
0DO WHILE (SSK.EOF = '0'
        &  SSK.REC_TYPE ¬= '99');    /* WHILE NOT EOF                */
0  CALL RCBBCZM_INIT_COUNTRY;
0  DO WHILE (SUBSTR (STRING (PREV_SSK), 1, 6)  /* WHILE SAME COUNTRY */
             = SUBSTR (STRING (SSK), 1, 6));
      SELECT;
       WHEN(STRING(SK0) < STRING(SK1)) /* SOURCE S1 SMALLEST         */
          DO;
           CALL R030_INSERT_RCBBS16;
           CALL RCBBCZM_CALC1;
           CALL R010_READ0;
          END;
       WHEN(STRING(SK0) > STRING(SK1)) /* SOURCE S2 SMALLEST         */
          DO;
           CALL R031_INSERT_RCBBS26;
           CALL RCBBCZM_CALC2;
           CALL R011_READ1;
          END;
       OTHERWISE DO;                 /* KEYS ARE EQUAL               */
           CALL R030_INSERT_RCBBS16;
           CALL RCBBCZM_CALC1;
           CALL R010_READ0;
           CALL RCBBCZM_CALC2;
           CALL R011_READ1;
          END;
      END;

     CALL RCBBCZM_SIC;               /* SET ITERATION CONDITIONS     */
   END;
0      CALL RCBBCZM_END_COUNTRY;                      /* END COUNTRY */
0END;

 END PROCESS_CA0_2_5_8_B;
