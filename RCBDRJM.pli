                     IF FILE_RCBDRJ0.CDBSCHG Â¬= 'D'                     08350021
                     THEN                                               08360021
                       IOAREA_OUT.CDBSCHG = '';                         08370021
                     IOAREA_OUT.FDTEUEX = '0';                          08380021
