   IF (RCBBIF0_TRAIL.SORTKEY = HIGH(15)                                 06010000
    |  RCBBIF0_TRAIL.SORTKEY = '999999999999999')         /* RA01236 */ 06020000
    &  EOF_RCBBIF0                                                     06030000
   THEN                                                                 06040000
     CALL RCBBIFM_PROCES_TRAILER;                                       06050000
