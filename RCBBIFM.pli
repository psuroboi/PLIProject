                                   /* MORE RECORDS FOR THIS COUNTRY? */ 06070000
     DO WHILE (((RCBBIF0_CUST.COUNTRY_NUMBER = WRK_CTRY_FILE_PREV)      06080000
         &      EOF_RCBBIF0)                                           06090000
         &    ((RCBBIF0_CUST.COUNTRY_NUMBER <= WRK_CTRYNO)              06100000
         |      SWITCH_END_CTY));                                       06110000
