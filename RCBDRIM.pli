           END;                                                         61750000
        END;                                                            61760000
        ELSE                                                            61770000
        DO;                                                             61780000
         WRK_BAU_PI_SRCC0MD.PI_PRIORITY     = 'Y';                      61790000
         CALL R511_FILL_CURRENT_BAU_INFO;                               61800000
        END;                                                            61810000
       END;                                                             61820000
       WHEN(WRK_PI_FLAG.SW_PI_CTR)               /* PI WARR & PI CTR */ 61830000
       DO;                                                              61840000
        IF PARM_RCB005.PI_TO_RCMS = 'Y' THEN                            61850000
         DO;                                                            61860000
