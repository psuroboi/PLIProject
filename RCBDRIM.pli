          DO N = 1 TO CTR_COUNT BY 1;                                   61110000
           IF ((SAVED_CTR_PI(N).EFFECTIVE_DATE <= DATE_ARRAY(P)) &      61120000
              (SAVED_CTR_PI(N).END_DATE >= DATE_ARRAY(P))) THEN         61130000
