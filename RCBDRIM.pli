     WHEN (0);                                                          93260000
     OTHERWISE                                                          93270000
       DO;                                                              93280000
         CALL RCBDRIM_ERROR (ADDR (RETURN_AREA_BB.RETURN_SQL),          93290000
                             'DB2',                                     93300000
                             '#074');                                   93310000
       END;                                                             93320000
