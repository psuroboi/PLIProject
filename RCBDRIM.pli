 /* ORIGINAL BAU RECORD PRESENT IN USER_FILE_BAU                      */60470000
 /* ORIGINAL PI RECORD PRESENT IN USER_FILE_PI                        */60480000
 /*********************************************************************/60490000
 R730_APPLY_PRIORITY: PROC;                                             60500000
                                                                        60510000
                                                                        60520000
 DCL P     FIXED BIN(31);                                               60530000
 DCL N     FIXED BIN(31);                                               60540000
                                                                        60550000
