               END;                                   /* EOC PTM1072 */ 03290005
           END;                                                         03300005
         IF PARM_RCBABN0.CAGAA Â¬= PARM_RCBABN0.CAGXA                    03310005
         THEN                                                           03320005
           DO;                                                          03330005
             CALL RCBAMHM_CREATE_CRT0380;                               03340005
             CALL RCBAMHM_WRITE_CRT0380;                                03350005
