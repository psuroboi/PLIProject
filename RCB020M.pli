   TB_RCB001.NUMBER            = 'RCB001';                              01660000
   TB_RCB001.PROC_ID           =  WRK_ARRAY.PROC_ID (WRK_IDX);          01670000
   TB_RCB001.CTRYNO            =  RUN_DATE_AREA.CTRYNO;                 01680000
